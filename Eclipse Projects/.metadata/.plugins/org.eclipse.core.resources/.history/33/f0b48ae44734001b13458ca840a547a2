package com;

import java.util.HashMap;
import java.util.LinkedHashMap;

public class MapDemo {

	public static void main(String[] args) {
		//HashMap hm = new HashMap();
		LinkedHashMap hm = new LinkedHashMap();
		hm.put(2, "Ravi");
		hm.put(1,"Ramesh");
		hm.put(3, "Rajesh");
		System.out.println(hm);
		hm.put(1, "Raju");
		hm.put(4, "Ravi");
		System.out.println(hm);
		
	}

}
